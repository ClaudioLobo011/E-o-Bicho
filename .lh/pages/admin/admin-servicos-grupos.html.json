            "content": "<!DOCTYPE html>\r\n<html lang=\"pt-BR\">\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"/>\r\n  <title>Admin — Cadastro de Grupo de Serviço</title>\r\n\r\n  <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css\">\r\n  <link rel=\"stylesheet\" href=\"../../src/output.css\">\r\n</head>\r\n<body class=\"bg-gray-100\">\r\n  <!-- Header Admin -->\r\n  <div id=\"admin-header-placeholder\"></div>\r\n\r\n  <main class=\"container mx-auto px-4 py-8 min-h-screen\">\r\n    <div class=\"grid grid-cols-1 md:grid-cols-4 gap-8\">\r\n      <!-- Sidebar Admin -->\r\n      <aside class=\"md:col-span-1\">\r\n        <div id=\"admin-sidebar-placeholder\"></div>\r\n      </aside>\r\n\r\n      <!-- Conteúdo -->\r\n      <section class=\"md:col-span-3\">\r\n        <div class=\"bg-white rounded-lg shadow p-6\">\r\n          <!-- Título -->\r\n          <div class=\"flex flex-col md:flex-row md:items-center md:justify-between gap-4 mb-6\">\r\n            <div class=\"flex items-center gap-3\">\r\n              <i class=\"fas fa-folder-tree text-gray-400 text-2xl\"></i>\r\n              <h1 class=\"text-2xl font-bold text-gray-800\">Cadastro de Grupo de Serviço</h1>\r\n            </div>\r\n            <a href=\"/pages/admin/admin-servicos-grupos.html\" class=\"text-sm text-gray-500 hover:text-gray-700 underline\">Atualizar</a>\r\n          </div>\r\n\r\n          <!-- Formulário -->\r\n          <form id=\"grupo-form\" class=\"space-y-4\">\r\n            <input type=\"hidden\" id=\"grupo-id\">\r\n\r\n            <div>\r\n              <label for=\"grupo-nome\" class=\"block text-sm font-medium text-gray-700 mb-1\">Nome do grupo</label>\r\n              <input id=\"grupo-nome\" type=\"text\" required maxlength=\"80\"\r\n                     class=\"w-full border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary/40\"\r\n                     placeholder=\"Ex.: Banho\">\r\n            </div>\r\n\r\n            <div>\r\n              <label class=\"block text-sm font-medium text-gray-700 mb-1\">Tipos de funcionário permitidos</label>\r\n              <div id=\"grupo-tipos\" class=\"grid grid-cols-2 gap-2\">\r\n                <label class=\"inline-flex items-center gap-2\">\r\n                  <input type=\"checkbox\" value=\"esteticista\" class=\"rounded border-gray-300\">\r\n                  <span>Esteticista</span>\r\n                </label>\r\n                <label class=\"inline-flex items-center gap-2\">\r\n                  <input type=\"checkbox\" value=\"veterinario\" class=\"rounded border-gray-300\">\r\n                  <span>Veterinário</span>\r\n                </label>\r\n                <label class=\"inline-flex items-center gap-2\">\r\n                  <input type=\"checkbox\" value=\"vendedor\" class=\"rounded border-gray-300\">\r\n                  <span>Vendedor</span>\r\n                </label>\r\n                <label class=\"inline-flex items-center gap-2\">\r\n                  <input type=\"checkbox\" value=\"gerente\" class=\"rounded border-gray-300\">\r\n                  <span>Gerente</span>\r\n                </label>\r\n              </div>\r\n              <p class=\"text-xs text-gray-500 mt-1\">Selecione um ou mais tipos.</p>\r\n            </div>\r\n\r\n            <div>\r\n              <label for=\"grupo-comissao\" class=\"block text-sm font-medium text-gray-700 mb-1\">Comissão geral (%)</label>\r\n              <input id=\"grupo-comissao\" type=\"number\" inputmode=\"decimal\" min=\"0\" max=\"100\" step=\"0.01\" value=\"0\"\r\n                     class=\"w-40 border border-gray-300 rounded-lg px-3 py-2 focus:outline-none focus:ring-2 focus:ring-primary/40\">\r\n            </div>\r\n\r\n            <div class=\"flex items-center gap-3 pt-2\">\r\n              <button type=\"submit\" class=\"inline-flex items-center gap-2 bg-primary text-white px-4 py-2 rounded-lg hover:opacity-90 transition\">\r\n                <i class=\"fas fa-save\"></i>\r\n                <span id=\"grupo-submit-label\">Salvar</span>\r\n              </button>\r\n              <button type=\"button\" id=\"grupo-cancelar\" class=\"hidden text-sm text-gray-600 hover:text-gray-800 underline\">Cancelar edição</button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n\r\n        <!-- Lista -->\r\n        <div class=\"bg-white rounded-lg shadow p-6 mt-6\">\r\n          <div class=\"flex items-center gap-3 mb-4\">\r\n            <i class=\"fas fa-list text-gray-400\"></i>\r\n            <h2 class=\"text-lg font-bold text-gray-800\">Grupos cadastrados</h2>\r\n          </div>\r\n\r\n          <div class=\"overflow-x-auto\">\r\n            <table class=\"min-w-full text-sm\">\r\n              <thead>\r\n                <tr class=\"text-left text-gray-500\">\r\n                  <th class=\"px-3 py-2\">Nome</th>\r\n                  <th class=\"px-3 py-2\">Tipos permitidos</th>\r\n                  <th class=\"px-3 py-2\">Comissão (%)</th>\r\n                  <th class=\"px-3 py-2 w-36\">Ações</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody id=\"grupo-tbody\" class=\"divide-y divide-gray-200\"></tbody>\r\n            </table>\r\n          </div>\r\n          <p id=\"grupo-empty\" class=\"text-gray-400 text-sm italic hidden mt-3\">Nenhum grupo cadastrado.</p>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </main>\r\n\r\n  <!-- Modais compartilhados -->\r\n  <div id=\"modal-placeholder\"></div>\r\n  <div id=\"confirm-modal-placeholder\"></div>\r\n\r\n  <!-- Footer Admin -->\r\n  <div id=\"admin-footer-placeholder\"></div>\r\n\r\n  <!-- Base path relativo à pasta /pages -->\r\n  <script>var basePath = '../../';</script>\r\n\r\n  <!-- Core -->\r\n  <script src=\"../../scripts/core/config.js\"></script>\r\n  <script src=\"../../scripts/core/ui.js\"></script>\r\n  <script src=\"../../scripts/core/main.js\"></script>\r\n\r\n  <!-- (Se houver) Guarda do Admin -->\r\n  <script src=\"../../scripts/admin/admin.js\"></script>\r\n\r\n  <!-- Página -->\r\n  <script src=\"../../scripts/admin/admin-servicos-grupos.js\"></script>\r\n</body>\r\n</html>"
